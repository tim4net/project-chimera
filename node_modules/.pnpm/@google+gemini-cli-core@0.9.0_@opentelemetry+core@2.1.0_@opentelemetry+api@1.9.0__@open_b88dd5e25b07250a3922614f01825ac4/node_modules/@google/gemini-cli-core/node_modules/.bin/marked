#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules/marked/bin/node_modules:/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules/marked/node_modules:/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules:/srv/project-chimera/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules/marked/bin/node_modules:/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules/marked/node_modules:/srv/project-chimera/node_modules/.pnpm/marked@15.0.12/node_modules:/srv/project-chimera/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../marked@15.0.12/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../../marked@15.0.12/node_modules/marked/bin/marked.js" "$@"
fi
