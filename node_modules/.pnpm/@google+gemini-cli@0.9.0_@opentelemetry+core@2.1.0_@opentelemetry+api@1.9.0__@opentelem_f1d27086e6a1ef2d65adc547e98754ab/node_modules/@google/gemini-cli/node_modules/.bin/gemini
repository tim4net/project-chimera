#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/gemini-cli/dist/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/gemini-cli/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules:/srv/project-chimera/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/gemini-cli/dist/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/gemini-cli/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules/@google/node_modules:/srv/project-chimera/node_modules/.pnpm/@google+gemini-cli@0.9.0_@opentelemetry+core@2.1.0_@opentelemetry+api@1.9.0__@opentelem_f1d27086e6a1ef2d65adc547e98754ab/node_modules:/srv/project-chimera/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
